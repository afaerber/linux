/*
 * Device tree for STM32F429I-DISCO
 */

/dts-v1/;
#include "stm32f429.dtsi"

/ {
	model = "ST STM32F4 Discovery kit for STM32F429";
	compatible = "st,stm32f429i-disco";

	chosen {
		bootargs = "console=ttystm3,115200n8 earlyprintk clk_ignore_unused";
	};

	memory {
		reg = <0x90000000 0x00800000>;
	};
};

&usart3 {
	status = "okay";
};
