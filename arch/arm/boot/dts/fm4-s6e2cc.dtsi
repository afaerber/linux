#include "fm4.dtsi"

/ {
	soc {
		/* sram0 offset differs with size */

		sram1: sram@20038000 {
			compatible = "mmio-sram";
			reg = <0x20038000 0x8000>;
			#address-cells = <1>;
			#size-cells = <1>;
			ranges = <0 0x20038000 0x8000>;
		};

		sram2: sram@20040000 {
			compatible = "mmio-sram";
			reg = <0x20040000 0x8000>;
			#address-cells = <1>;
			#size-cells = <1>;
			ranges = <0 0x20040000 0x8000>;
		};
	};
};
