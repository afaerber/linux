/dts-v1/;

#include "fm4-s6e2cca.dtsi"

/ {
	model = "Cypress SK-FM4-176L-S6E2CC-ETH";
	compatible = "cypress,fm4";

	aliases {
		serial0 = &mfs0;
	};

	chosen {
		bootargs = "console=ttyFM0,115200n8 earlycon ignore_loglevel ihash_entries=64 dhash_entries=64 uclinux.physaddr=0x00180000 root=/dev/mtdblock0 init=/sbin/init";
		stdout-path = "serial0:115200n8";
	};

	memory@60000000 {
		reg = <0x60000000 0x00200000>;
	};

	main_osc: main-osc-clk {
		compatible = "fixed-clock";
		clock-frequency = <4000000>;
		#clock-cells = <0>;
	};

	sub_osc: sub-osc-clk {
		compatible = "fixed-clock";
		clock-frequency = <32768>;
		#clock-cells = <0>;
	};
};

&cgu {
	clocks = <&main_osc>, <&sub_osc>;
	clock-names = "CLKMO", "CLKSO";
};

&mfs0 {
	status = "okay";
};
